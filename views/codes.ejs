<% include ./partials/header %>

<h1>codes</h1>
<a href="/codes/new">New Code</a>
<a href="/codes/upload">Upload Code</a>

<div>
  <% if(codes) { %>
    <% codes.forEach(code => { %>
      <%- code.svg %>
      <p><%= code.title %></p>
      <p><%= code.text %></p>
    <% }); %>
  <% } %>
</div>

